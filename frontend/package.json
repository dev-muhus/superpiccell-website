  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:reset": "tsx src/db/reset.ts",
    "test:api": "cross-env TEST_MODE=true jest --runInBand",
    "test:api:ci": "cross-env TEST_MODE=true CI=true jest --runInBand --ci",
    "test:db:start": "docker compose up -d test-db",
    "test:db:stop": "docker compose stop test-db",
    "test:db:reset": "docker compose rm -f test-db && docker compose up -d test-db"
